    },
    "user_reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-review.user-review",
      "mappedBy": "arena"
    },
    "faq": {
      "type": "component",
      "repeatable": true,
      "component": "blocks.faq"
    }
  }
